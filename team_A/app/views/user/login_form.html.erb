<div class = "login-wrapper">
  <div class = "login-title">
    <h1>ログイン</h1>
  </div>
  <%= form_for :session, :url=> user_login_form_path do |f| %>
  <div class = "login-user_email">
    <p>メールアドレスを入力</p>
    <%= f.text_field :email %>
  </div>
  <div class = "login-user_email">
    <p>パスワードを入力</p>
    <%= f.text_field :password %>
  </div>
  <%= f.submit 'ログインする' %>>
  <% end %>
</div>
